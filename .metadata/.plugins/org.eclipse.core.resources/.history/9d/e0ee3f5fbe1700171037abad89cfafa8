public class Solution {
    public int maxSubArray(int[] nums) {
        int pre=0;
        int max=Integer.MIN_VALUE;
        
        for(int i=0;i<nums.length;i++){
            if(pre>0){
                max=Math.max(pre+nums[i],max);
                pre=pre+nums[i];
            }
            else{
                max=Math.max(nums[i],max);
                pre=nums[i];
            }
        }
        return max;
    }
}