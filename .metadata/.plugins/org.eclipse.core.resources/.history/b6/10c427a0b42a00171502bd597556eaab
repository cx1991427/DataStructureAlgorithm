import java.io.*;
import java.net.*;
import java.util.*;

public class Test extends Thread{
	
	public static void main(String[] args){
		
		
		
		String str="xiangcao";
		char[] arr=str.toCharArray();
		for(char ch:arr){
			if(ch=='i'|| ch=='o'){
				ch='#';
			}
		}
		System.out.println(arr);
	}
}






