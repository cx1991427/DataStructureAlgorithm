		Node node0=new Node(0,3);
		Node node1=new Node(1,3);
		Node node2=new Node(2,3);
		Node node3=new Node(3,3);
		
		node0.child[0]=node1;
		node0.child[1]=node2;
		node0.child[2]=node3;
		
		node1.child[0]=node0;
		node1.child[1]=node2;
		node1.child[2]=node3;
		
		node2.child[0]=node0;
		node2.child[1]=node1;
		node2.child[2]=node3;
		
		node3.child[0]=node0;
		node3.child[1]=node1;
		node3.child[2]=node2;
		
		
		
	}
