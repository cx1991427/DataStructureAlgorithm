import java.util.*;

public class Test {

	public static void main(String[] args) {
		Integer[] arr={1,3,2};
		Arrays.sort(arr,new Comparator<Integer>(){
			public int compare(Integer e1, Integer e2) {
				// TODO Auto-generated method stub
				return e2-e1;
			}
		});
		
		for(int x:arr){
			System.out.println(x);
		}
	}

}
