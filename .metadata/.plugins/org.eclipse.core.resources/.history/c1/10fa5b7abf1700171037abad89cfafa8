public class Solution {
    public int maxSubArray(int[] nums) {
        int pre=0;
        int max=Integer.MIN_VALUE;
        int[] res=new int[2];
        
        for(int i=0;i<nums.length;i++){
            if(pre>0){
                max=Math.max(pre+nums[i],max);
                if(pre+nums[i]>max){
                	res[1]=i;
                }
                pre=pre+nums[i];
            }
            else{
            
//                max=Math.max(nums[i],max);
            	if(nums[i]>max){
            		nums[0]=i;
            		nums[1]=i;
            	}
                pre=nums[i];
            }
        }
        return max;
    }
}